{
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "dependencies": {
    "kind-of": "^6.0.2",
    "shallow-clone": "^3.0.0"
  },
  "deprecated": false,
  "description": "Recursively (deep) clone JavaScript native types, like Object, Array, RegExp, Date as well as primitives.",
  "devDependencies": {
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/jonschlinkert/clone-deep",
  "license": "MIT",
  "main": "index.js",
  "name": "clone-deep",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonschlinkert/clone-deep.git"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "is-plain-object",
        "isobject",
        "kind-of",
        "shallow-clone"
      ]
    },
    "lint": {
      "reflinks": true
    }
  },
  "version": "4.0.0"
}